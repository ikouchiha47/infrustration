
{
    "family": "talon-server-service",
    "containerDefinitions": [
        {
            "name": "talon-server-service",
            "image": "${AWS_ACCOUNT}.dkr.ecr.ap-south-1.amazonaws.com/${IMAGE}",
            "cpu": 128,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 9090,
                    "protocol": "tcp"
                }
            ],
            "command": [
                "/opt/talon/server"
            ],
            "essential": true
        }
    ]
}
